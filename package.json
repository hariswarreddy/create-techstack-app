{
  "name": "create-techstack-app",
  "version": "2.1.0",
  "description": "Interactive CLI to scaffold full-stack applications with your choice of frontend (React, Vue, Next.js) and backend (Express, Go) - Now with TypeScript backends!",
  "main": "dist/index.js",
  "bin": {
    "create-techstack-app": "dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli",
    "scaffolding",
    "fullstack",
    "react",
    "vue",
    "nextjs",
    "express",
    "mongodb",
    "postgresql",
    "golang",
    "create-app",
    "boilerplate",
    "generator",
    "typescript",
    "tailwindcss"
  ],
  "author": "Hariswar Reddy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hariswarreddy/create-techstack-app"
  },
  "bugs": {
    "url": "https://github.com/hariswarreddy/create-techstack-app/issues"
  },
  "homepage": "https://github.com/hariswarreddy/create-techstack-app#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "inquirer": "^9.2.23",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "templates"
  ]
}
